
{
    "name": "newpage",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Minimalistic New Tab Page",
    "permissions": [
        "topSites",
        "chrome://favicon/",
        "<all_urls>",
        "tabs",
        "storage",
        "unlimitedStorage"
    ],
    "background": {
        "scripts": [
            "bower_components/jquery/dist/jquery.min.js",
            "bower_components/RainbowColor/lib/rainbow.min.js",
            "bower_components/color-thief/js/color-thief.js",
            "background.js"
            ]
    },
    "chrome_url_overrides":
            {
                "newtab": "newpage.html"
            },
    "web_accessible_resources": [
        "newpage.html"
    ]
}